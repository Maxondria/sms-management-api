{
    "require": [
      "@babel/register",
      "dotenv/config"
    ],
	"reporter": [
		"lcov",
		"text"
	],
	"include": [
		"app/**/*.js"
	],
	"exclude": [
		"node_modules",
		"bin",
		"coverage",
		"test",
		"**/config/**",
		"**/helpers/models.**.js",
		"**/helpers/index.js"
	],
	"sourceMap": false,
	"instrument": false,
	"cache": true,
	"all": true,
	"extension": [
		".js"
	]
}
